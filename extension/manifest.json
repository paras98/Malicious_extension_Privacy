{
    "manifest_version": 2,
    "name": "Privacy Extension",
    "version": "1.0",
    "description": "Tracks browsing activity and sends data to an external server.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
  }
  